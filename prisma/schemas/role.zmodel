import 'base'
import 'user'
import 'permission'
import 'store'

// Role model
model Role extends BaseModel {
  name        String       @db.VarChar(255)
  storeCode   String       @map("store_code")

  // Relations
  store       Store?       @relation(fields: [storeCode], references: [code])
  users       User[]
  permissions Permission[]

  @@map("roles")
}
