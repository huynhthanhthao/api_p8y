import 'base'
import 'store'
import 'supplier'

model SupplierGroup extends BaseModel {
  name      String     @db.VarChar(255)
  note      String?    @db.VarChar(500) @default("")
  storeCode String     @map("store_code") @db.VarChar(50)

  suppliers Supplier[]
  store     Store      @relation(fields: [storeCode], references: [code])

  @@map("supplier_groups")
}
