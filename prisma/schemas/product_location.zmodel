import 'base'
import 'product'
import 'branch'

model ProductLocation extends BaseModel {
  name     String    @db.VarChar(255)
  branchId String    @map("branch_id") @db.Uuid
  
  // Relations
  branch   Branch    @relation(fields: [branchId], references: [id])
  products Product[]

  @@index([branchId, deletedAt])
  @@map("stock_locations")
}